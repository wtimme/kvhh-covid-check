table.table
  thead
    tr
      th(scope="col") Auftrags-Nr.
      th(scope="col") Befund
      th(scope="col") Auftragsdatum
  tbody
    each row in rows
      tr
        th(scope="row") #{row.Code}
        td #{row.Result}
        td #{row.Date}

